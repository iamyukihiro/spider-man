#!/usr/bin/env php
<?php

require __DIR__.'/../vendor/autoload.php';

use Symfony\Component\Console\Application;
use Goreboothero\SpiderMan\Command\GetFishingEnvironmentalDataCommand;

$app = new Application('SpiderMan for CLI Application', '0.0.1');
$app->addCommands([new GetFishingEnvironmentalDataCommand()]);

$app->run();